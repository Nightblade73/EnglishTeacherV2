{
  "manifest_version": 2,

  "name": "GTmetrix Analyzer Plugin",
  "description": "This extension will analyze a page using GTmetrix",
  "version": "1.0",

  "background": {
    "persistent": false,
    "page": "Views/Home/background.html"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "Views/Home/autorization.html"
  },

  "permissions": [
    "activeTab"
  ]
}
